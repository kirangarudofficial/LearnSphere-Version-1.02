# Production Environment Variables
# These should be set in your production deployment platform

# API Configuration
VITE_API_URL=https://api.learninghub.com/api
VITE_APP_ENV=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_LIVE_CLASSES=true

# External Services
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_STRIPE_PUBLIC_KEY=pk_live_xxxxxxxxxxxxx

# Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,jpg,png

# Session Configuration
VITE_SESSION_TIMEOUT=3600000

# Debug Mode (should be false in production)
VITE_DEBUG_MODE=false
